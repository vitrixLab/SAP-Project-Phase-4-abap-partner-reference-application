{
  "_version": "1.65.0",
  "sap.app": {
    "id": "visitors",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.17.5",
      "toolsId": "98b98724-18bd-4b9f-8c05-5d9859a0e747"
    },
    "dataSources": {
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zpra_mf_ui_visitor_o4/srvd/sap/zpra_mf_visitor/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZPRA_MF_VSTR-display": {
          "semanticObject": "ZPRA_MF_VSTR",
          "action": "display",
          "title": "{{ZPRA_MF_VSTR-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://visits",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "visitors.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "VisitorList",
          "target": "VisitorList"
        },
        {
          "pattern": "Visitor({key}):?query:",
          "name": "VisitorObjectPage",
          "target": "VisitorObjectPage"
        },
        {
          "pattern": "Visitor({key})/_Visits({key2}):?query:",
          "name": "VisitsObjectPage",
          "target": "VisitsObjectPage"
        }
      ],
      "targets": {
        "VisitorList": {
          "type": "Component",
          "id": "VisitorList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Visitor",
              "variantManagement": "Page",
              "navigation": {
                "Visitor": {
                  "detail": {
                    "route": "VisitorObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable",
                    "selectAll": true
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "VisitorObjectPage": {
          "type": "Component",
          "id": "VisitorObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Visitor",
              "navigation": {
                "_Visits": {
                  "detail": {
                    "route": "VisitsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "_Visits/@com.sap.vocabularies.UI.v1.LineItem#VisitorMusicFestivals": {
                  "tableSettings": {
                    "selectAll": true,
                    "creationMode": {
                      "inlineCreationRowsHiddenInEditMode": true,
                      "name": "InlineCreationRows"
                    }
                  }
                }
              }
            }
          }
        },
        "VisitsObjectPage": {
          "type": "Component",
          "id": "VisitsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Visitor/_Visits"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}